{
  "extends": [
    "./node_modules/kyt/config/.eslintrc.base.json"
  ],

  "env": {
  },

  "plugins": [
  ],

  "rules": {
      "react/prefer-es6-class": 1,
      "object-shorthand": 1,
      "comma-dangle": 0,
      "indent": ["error", 4, { "SwitchCase": 2, "VariableDeclarator": 2, "outerIIFEBody": 2 }],
      "react/jsx-indent-props": ["error", 4],
      "react/jsx-indent": ["error", 4]
  }
}
